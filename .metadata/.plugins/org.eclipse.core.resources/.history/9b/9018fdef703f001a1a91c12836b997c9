package lv.danamark.ArraysTest;

import java.util.Scanner;

public class ScannerArraysTest {

	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int[] arr = new int [2];
		
		int ievad1 = sc.nextInt();
		int ievad2 = sc.nextInt();
		
		System.out.println("ievadi araya 1 vert: ");
		arr[0] = sc.nextInt();
		System.out.println("ievadi araya 2 vert: ");
		arr[1] = sc.nextInt();
		
		
		System.out.println("arraya 1 v " +ievad1+ " arraya 2 v "+ievad2);
		
		
		
		
		
		
		
	}

}
