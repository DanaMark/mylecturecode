package lv.dana;

public class LearnMethods {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		getMyName();
		getMyName();
		getMyName();
		System.out.println("_____________________");	
		getMyAge();
		}
		
	
	public static void getMyName(){
		System.out.println("My name is Dana");
		
	}
	public static void getMyAge(){
		getMyName();
		System.out.println("My age is 40");
		
	}
	public static void getMycity(){
	
}
}
