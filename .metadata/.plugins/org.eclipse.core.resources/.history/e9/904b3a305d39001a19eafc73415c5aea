
public class ControFlowIf {

	
	public static void main(String[] args) {
		
		int x = 12;
		int y = 22;
		
		if(x>y){
			System.out.println("X < Y");
		}
		
		if(x%2==0){
			System.out.println(" x ir paara skaitlis");
		}else{
			System.out.println(" x ir nepaara skaitlis");
			}
		
		//System.out.println("nav pareizi");
	}

}
