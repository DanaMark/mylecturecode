
public class PrimType {

	
	public static void main(String[] args) {
		//Numeric data primitive type
		int integer = 200;
		long longInteger = 02024040240L;
		short shortInteger = 325;
		byte byteInteger = 125;
		//Floating point  type real numbers
		double doubleFloatNumber = 0.5d;
		float floatNumber = 0.1241752f;
		
		//Characters
		char charSymbol = '$';
		char charSymbolAsNumber = 63;
		char charD = 68;
		char charA = 65;
		char charN = 78;
		System.out.println("" + charD + charA + charN + charA);
		System.out.println(charD+""+charA+""+charN+""+charA);
		
//		Booleans
		boolean booleanTrueFalse = false;
		System.out.println(booleanTrueFalse);
		
		
		
		
		
		
		
		
		

	}

}
