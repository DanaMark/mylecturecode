
public class PrimType {

	
	public static void main(String[] args) {
		//Numeric data primitive type
		int integer = 200;
		long longInteger = 02024040240L;
		short shortInteger = 325;
		byte byteInteger = 125;
		//Floating point  type real numbers
		double doubleFloatNumber = 0.5d;
		float floatNumber = 0.1241752f;
		
		//Characters
		char charSymbol = '$';
		char CharSymbolAsNumber = 43;
		System.out.println("Char : " + charsSymbolAsNumber);
		
		
		
		
		
		
		

	}

}
