
public class ForEach {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int[] marks = {76, 23, 54, 43, 98, 65, 34};
		//for each lloops
		for(int mark : marks){
			System.out.println(mark);
			

	}
System.out.println("\n"+"+++++++++");
		
		
		System.out.print(" ");
		for(int i = 0; i < marks.length; i++){
			System.out.println(marks[i]);
	}
}
}
