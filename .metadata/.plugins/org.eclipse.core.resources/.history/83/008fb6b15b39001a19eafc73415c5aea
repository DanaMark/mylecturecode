
public class ControFlowIf {

	
	public static void main(String[] args) {
		
		int x = 11;
		int y = 22;
		
		if(x>y){
			System.out.println("X < Y");
		}
		
		if(x%2==0){
			System.out.println("paara skaitlis");
		}
		else 
			System.out.println("nepaara skaitlis");
		
		//System.out.println("nav pareizi");
	}

}
